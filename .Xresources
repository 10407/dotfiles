! Dylans Xresources

Xft.lcdfilter: lcddefault
Xft.antialias: true
Xft.autohint: 0
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.dpi: 96

! Set the font
! URxvt*font: xft:monospace:pixelsize=11
URxvt*font: xft:Fira Mono:size=11
URxvt*boldFont: xft:Fira Mono:size=11
URxvt*lineSpace: 1

! Disable scrollbar
URxvt.scrollBar: false

! Set terminal padding
! URxvt.internalBorder: 15

! Colors {{{

! special
*foreground:   #f0f0f0
*background:   #1c1c1c
*cursorColor:  #f0f0f0

! black
! Second black is grey
*color0:       #232c33
*color8:       #70838c

! red
*color1:       #99736e
*color9:       #99736e

! green
*color2:       #78a090
*color10:      #78a090

! yellow
*color3:       #bfb7a1
*color11:      #bfb7a1

! blue
*color4:       #7c9fa6
*color12:      #7c9fa6

! magenta
*color5:       #BF9C86
*color13:      #BF9C86

! cyan
*color6:       #99BFBA
*color14:      #99BFBA

! white
*color7:       #f0f0f0
*color15:      #f0f0f0

! }}}
